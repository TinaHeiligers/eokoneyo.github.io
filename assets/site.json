---
layout: null
---

[{% for post in site.posts %}
{
"title"    : "{{ post.title }}",
"url"     : "{{ post.url }}",
"category": "{{ post.category }}",
"date"     : "{{ post.date | date: "%B %d, %Y" }}",
"content"  : "{{ post.excerpt | remove: '<p>' | remove: '</p>' | strip_newlines }}"
} {% if forloop.last %}{% else %},{% endif %}
{% endfor %}]
